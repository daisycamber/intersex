  "bugs": {
    "url": "https://github.com/semantic-ui-forest/forest-templates/issues"
  },
  "homepage": "https://github.com/semantic-ui-forest/forest-templates#readme",
  "devDependencies": {
    "codesandbox": "^1.3.6",
    "console-stamp": "^0.2.7",
    "prettier": "^1.15.3",
    "puppeteer": "^1.11.0",
    "sharp": "^0.21.3"
  }
